{
  "extends": [
    "tslint-microsoft-contrib/recommended"
  ],
  "rules": {
    "semicolon": [true, "never"],
    "trailing-comma": [true, "never"],
    "align": false,
    "import-name": false,
    "export-name": false,
    "no-cookies": false,
    "function-name": false,
    "no-relative-imports": false
  }
}